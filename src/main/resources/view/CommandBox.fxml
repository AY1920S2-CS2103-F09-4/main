<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>

<StackPane styleClass="stack-pane" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <HBox prefHeight="0.0">
      <children>
        <TextField fx:id="commandTextField" onAction="#handleCommandEntered" promptText="What can I help you with?" HBox.hgrow="ALWAYS" />
         <Button prefHeight="40.0" prefWidth="60.0" fx:id="enterBtn" alignment="CENTER" mnemonicParsing="false"
                 styleClass="button-enter"
                 text="Enter" HBox.hgrow="ALWAYS" />
      </children>
   </HBox>
</StackPane>
